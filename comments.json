    "books-state": {
      "projectType": "library",
      "root": "projects/books-state",
      "sourceRoot": "projects/books-state/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/books-state/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/books-state/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/books-state/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/books-state/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/books-state/**/*.ts",
              "projects/books-state/**/*.html"
            ]
          }
        }
      }
    },

